@{
    Layout = null;
}
<div id="body_print">
    <style type="text/css">
        .body-print {
            margin: 0 auto;
            font-family: "Times New Roman" !important;
            font-size: 12pt;
            color: #000;
            margin: 0px auto;
            max-width: 750px;
            background-color: #fff;
            padding-bottom: 50px;
        }

            .body-print div, .body-print p, .body-print td, .textdaudong p span {
                font-family: "Times New Roman" !important;
                font-size: 12pt !important;
            }

        .hiddenTable {
            background-color: #fff;
            border: 0.5pt solid #ccc;
        }

        .text_head {
            text-align: center;
            font-weight: bold;
        }

        .textheadh2 {
            font-weight: bold;
            text-transform: uppercase;
            margin-top: 2mm;
            padding-top: 6px;
        }

        .textdaudong {
            margin-left: 5mm;
        }

        .headertxt {
            text-align: center;
            font-weight: bold;
        }

        .tab_head {
            text-align: center;
        }

        .tdtxt {
            text-align: center;
        }

        #body_print p {
            margin: 0 0 2px;
        }
    </style>
    <div style="padding:0 40px; text-align:justify;padding-bottom:100px" class="body-print">
        <div class="text_head" id="print_header" style="height:60px;"></div>
        <div class="text_head" style="font-size:16pt; font-weight:bold;"><center>CỘNG HÒA XÃ HỘI CHỦ NGHĨA VIỆT NAM</center></div>
        <div class="text_head" style="font-weight:bold;"><center>Độc lập - Tự do - Hạnh phúc</center></div>
        <div class="text_head" style="font-weight:bold;"><center>-----------------</center></div>
        <div class="text_head" style="font-size:16pt; font-weight:bold;"><center>HỢP ĐỒNG QUẢNG CÁO TRÊN MẠNG INTERNET</center></div>
    </div>
</div>