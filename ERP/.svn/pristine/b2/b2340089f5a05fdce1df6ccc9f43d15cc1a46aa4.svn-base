.dropdown-content {
    display: none;
    text-align: left !important;
    background-color: #f9f9f9;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
}
.dropdown-content a{
   text-align: left !important;
   padding-left: 20px !important;
}

.dropdown-content li {
  position: relative;
  line-height: 26px;
  transition: all 0.3s ease;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -o-transition: all 0.3s ease; }

.dropdown-content li:hover {
  padding-left: 10px;
  color: #F6435B; }

.dropdown-content li:hover:before {
  content: "\f105";
    font-family: 'FontAwesome';
    line-height: 60px;
    position: absolute;
    left: 0;
    top: 0;
    font-size: 20px; }
.dropdown:hover .dropdown-content {
    display: block;
}


/*Pagination*/
.pagination {
    width: 100%;
    padding: 10px;
    padding-right: 25px;
}
.pagination .page {
   float: right;
}
.pagination .page li {
    display: block;
    float: left;
}
.pagination .page li a.active {
    background: #F5003B;
    border-color: #F5003B;
    color: #fff;
    font-weight: bold;
}
.pagination .page li a {
    display: block;
    float: left;
    margin-right: 3px;
    padding: 0 9px;
    border: 1px solid #f7f7f7;
    border-radius: 3px;
    color: #000;
}
.pagination .page li a:hover {
    background: #f6435b;
    border-color: #f6435b;
    color: #fff;
}
div.pagination a {
    cursor: pointer;
}
a.active {
    text-decoration: none!important;
}
.pagination a {
    padding: 0 10px;
    border: 1px solid #ebebeb;
    border-radius: 3px;
    height: 28px;
    line-height: 26px;
    text-transform: none;
    color: #6e6e6e;
}

.pagination a.prev, .pagination a.next {
    background: #f7f7f7;
}

