<div id="">
    <h4 class="title-group">Lịch sử khách hàng</h4>
    <div class="row-fluid">
        <div class="tabbable">
            <ul class="nav nav-tabs tab-color-blue" id="myTab">
                <li class="active">
                    <a data-toggle="tab" href="#tabWork">
                        <i class="icon-calendar"></i>
                        Công việc
                    </a>
                </li>
                <li class="">
                    <a data-toggle="tab" href="#tabContract">
                        <i class="icon-exchange"></i>
                        Hợp đồng
                    </a>
                </li>
                <li class="">
                    <a data-toggle="tab" href="#tabHistoryCall">
                        <i class="icon-legal"></i>
                        Lịch sử cuộc gọi
                    </a>
                </li>

            </ul>
            <div class="tab-content" style="padding:0;background-color:#fff">
                <div  id="tabWork" class="tab-pane active">
                    <div style="padding:10px;" >
                        @Html.Partial("tab/_tab_worklist")
                    </div>
                </div>
                <div id="tabContract" class="tab-pane">
                    #tabContract
                </div>
                <div id="tabHistoryCall" class="tab-pane">
                    @Html.Partial("tab/_tab_history_call")
                </div>
            </div>
        </div>
    </div>
</div>


