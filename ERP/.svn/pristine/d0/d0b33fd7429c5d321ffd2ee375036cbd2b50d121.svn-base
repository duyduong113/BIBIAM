.chosen-container .chosen-drop {
    z-index: 9999;
}

.typeahead {
    position: fixed;
}

.tags {
    width: 80%;
}

.form-horizontal .control-group {
    margin-bottom: 5px;
}

.k-edit-form-container {
    width: 100% !important;
}

.k-window-title {
    font-size: 14px;
}

.k-dropzone {
    background-color: whitesmoke;
    margin-bottom: 5px;
}

.k-upload-button {
    background-color: #2283c5 !important;
    color: white;
}

.k-dropzone em {
    color: #2283c5;
    visibility: visible;
}

.detail-popup-title {
    font-size: 14px;
    padding: 10px 10px 0px 10px;
}

.detail-popup-description {
    font-size: 12px;
    padding: 0px 10px 0px 10px;
}

.detail-popup-createdtime {
    font-size: 10px;
    padding: 0px 10px 10px 10px;
}

.k-grid-add {
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: #87b87f !important;
    background-repeat: repeat-x;
}

    .k-grid-add:hover {
        background-color: #5bb75b;
        color: #fff;
    }

.k-icon.k-add {
    display: none;
}

.k-icon.k-update {
    display: none;
}

.k-grid-save-changes {
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: #6fb3e0 !important;
    background-repeat: repeat-x;
}

    .k-grid-save-changes:hover {
        background-color: #6fb3e0;
        color: #fff;
    }

.k-grid-cancel-changes {
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: #a0a0a0 !important;
    background-repeat: repeat-x;
}



.k-grid-excel {
    background-color: #6fb3e0 !important;
    background-repeat: repeat-x;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
}

    .k-grid-excel:hover {
        background-color: #6fb3e0;
        color: #fff;
    }

.k-icon.k-cancel {
    display: none;
}

.chosen-container {
    font-size: 11px;
}

.form-actions {
    margin-top: 5px;
    margin-bottom: 0px;
}

.modal-backdrop {
    z-index: 10009;
}

.modal {
    z-index: 10010;
}

.k-grid-content .text-box {
    height: 28px;
}

.k-grid-header th.k-header, .k-filter-row th {
    border-left-style: ridge;
}

.modal-body {
    max-height: 500px;
}

.modal {
    width: 580px;
}

.in-grid-form .control-label {
    width: 100px;
}

.in-grid-form .controls {
    margin-left: 110px;
}

span.k-icon.k-edit {
    display: none;
}

a.k-primary.k-grid-update {
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: #6fb3e0 !important;
    background-repeat: repeat-x;
}

a.k-button-icontext.k-grid-cancel {
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: #a0a0a0 !important;
    background-repeat: repeat-x;
}

.widget-box {
    padding: 2px 0px 7px 0px !important;
}

.btn.btn-small > .caret {
    border-width: 6px !important;
}

button.btclose {
    float: right;
    font-size: 22px;
    text-shadow: 0 1px 0 #fff;
    opacity: 0.2;
    background: transparent;
    border: 0;
    margin-top: -3px;
}

.chosen-container .chosen-results {
    max-height: 100px !important;
}

.form-footer-custom {
    background: #F7F7F7;
    height: 30px;
    padding-top: 8px;
    padding-bottom: 8px;
    margin: -6px;
}

.modal-footer {
    padding: 10px 15px 10px !important;
}

.daterangepicker_start_input, .daterangepicker_end_input {
    display: none;
}

.cancelBtn {
    margin-left: 14px;
}
/*btn remove date*/
.btncleardatetimepicker {
    position: relative;
}

.btncleardatetimepicker .remove-date {
    position: absolute;
    top: 3px;
    left: 155px;
    width: 20px;
    height: 20px;
}

.btncleardatetimepicker .remove-date a {
    color: #F44336;  /*#AAAAAA;*/
    font-size: 1.2em;
    display: none;
}

.btncleardatetimepicker:hover a {
    display: block;
}
/*end btn*/
input[type="checkbox"] {
    margin-top: -4px;
}