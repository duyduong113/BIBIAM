                                <a href="@Url.Content("~/Merchant_Product/Export?type=0")" role="button" class="btn btn-primary default btn-sm btn-circle" data-toggle="modal"><i class="fa fa-file-excel-o"></i> Dowload template </a>
                                }
                            @if (export || all)
