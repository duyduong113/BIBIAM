<script type="text/x-kendo-template" id="EditTemplate">
    <form action="@Url.Content("~/Merchant_Product/CreateUpdateProduct")" method="post" id="EditForm" novalidate="novalidate">
        <input type="hidden" name="id" value="#=id#" />
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="actions">
                    @if (all || create || update)
                    {
                        <a onclick="clearForm()" class="btn default btn-circle"><i class="fa fa-ban" aria-hidden="true"></i> @Html.Raw(Resources.Global._cancel)</a>
                    }

                    #if(id>0&&ma_san_pham!=null){#
                    @if (all || create || update)
                    {
                    <button type="button" onclick="SaveChange()" class="btn blue btn-circle"><i class="fa fa-floppy-o" aria-hidden="true"></i> Lưu thay đổi</button>
                    }
                    #}else{#
                    @if (all || create || update)
                    {
                        <button type="button" onclick="SaveChange()" class="btn blue btn-circle"><i class="fa fa-floppy-o" aria-hidden="true"></i> Lưu</button>
                    }
                    #}#
                    @if (all || create || update)
                    {
                        <a onclick="saveAndClear()" class="btn green btn-circle"><i class="fa fa-floppy-o" aria-hidden="true"></i> @Html.Raw(Resources.Global._save_new)</a>
                    }
                    #if(id>0){#
                    @if (delete || all)
                    {  <button type="button" class="btn btn-circle btn-danger" onclick="deleteRow('#=id#')"><i class="fa fa-trash" aria-hidden="true"></i> @Html.Raw(Resources.Global._delete)</button>}
                    
                    #}#
                    #if(trang_thai_duyet!='@AllConstant.trang_thai_duyet.DA_DUYET'&&id>0){#
                        @if (isAdmin)
                        {
