                                <a href="@Url.Content("~/Merchant_Product/Export?type=1")" role="button" class="btn btn-primary default btn-sm btn-circle" data-toggle="modal"><i class="fa fa-file-excel-o"></i> Xuất dữ liệu </a>
                                }*@
                        </div>
                    </text>);
                    })
                                                        .Events(events =>
                                                        {
                                                            events.DataBound("dataBound");
                                                        })
                            //.Groupable()
                            //.ColumnMenu()
                                                        .Sortable()
                                                        .Reorderable(reorderable => reorderable.Columns(true))
