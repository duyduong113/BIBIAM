                        </tr>
                        <tbody>
                            <tr>
                                <td style="padding:0 10px;text-align:center"><div style="width:196px;margin-top:50px" class="dotted">@ViewBag.itemAdditional.StaffSign</div></td>
                                <td style="padding:0 10px;text-align:center"><div style="width:196px;margin-top:50px" class="dotted">@ViewBag.itemAdditional.StaffTeamSign</div></td>
                                <td style="padding:0 10px;text-align:center"><div style="width:196px;margin-top:50px" class="dotted">@ViewBag.itemAdditional.StaffLeadSign</div></td>
